<?xml version="1.0" encoding="UTF-8"?>
<root>
  <name>STB Light</name>
  <slug>stb_light</slug>
  <icon>stb-light-theme.jpg</icon>
  <description>Light content boxes with colored captions.</description>
  <author>minimus</author>
  <author_url>http:\\www.simplelib.com</author_url>
  <jsStyles>
    <alert>
      <image>alert.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#FD3C35</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#FD3C35</color>
        <colorTo>#8D251D</colorTo>
      </caption>
    </alert>
    <black>
      <image>earth.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#3b3b3b</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#3b3b3b</color>
        <colorTo>#000000</colorTo>
      </caption>
    </black>
    <download>
      <image>download.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#78c0f7</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#78c0f7</color>
        <colorTo>#2e7cb9</colorTo>
      </caption>
    </download>
    <info>
      <image>info.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#a1ea94</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#a1ea94</color>
        <colorTo>#79b06e</colorTo>
      </caption>
    </info>
    <warning>
      <image>warning.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#FDE97D</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#FDE97D</color>
        <colorTo>#F09C08</colorTo>
      </caption>
    </warning>
    <grey>
      <image>gears.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#e3e3e3</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#e3e3e3</color>
        <colorTo>#ababab</colorTo>
      </caption>
    </grey>
    <custom>
      <image>heart.png</image>
      <color>#ffffff</color>
      <colorTo>#dddddd</colorTo>
      <fontColor>#000000</fontColor>
      <border>
        <width>0</width>
        <color>#f7cdf5</color>
      </border>
      <caption>
        <fontColor>#ffffff</fontColor>
        <color>#f7cdf5</color>
        <colorTo>#f77df1</colorTo>
      </caption>
    </custom>
  </jsStyles>
  <cssStyles>
    <alert>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>FD3C35</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>FD3C35</captionBgColor>
      <captionBgColorEnd>8D251D</captionBgColorEnd>
      <image>alert.png</image>
      <bigImg>alert.png</bigImg>
    </alert>
    <black>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>3B3B3B</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>3B3B3B</captionBgColor>
      <captionBgColorEnd>000000</captionBgColorEnd>
      <image>earth.png</image>
      <bigImg>earth.png</bigImg>
    </black>
    <download>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>78C0F7</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>78C0F7</captionBgColor>
      <captionBgColorEnd>2E7CB9</captionBgColorEnd>
      <image>download.png</image>
      <bigImg>download.png</bigImg>
    </download>
    <info>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>A1EA94</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>A1EA94</captionBgColor>
      <captionBgColorEnd>79B06E</captionBgColorEnd>
      <image>info.png</image>
      <bigImg>info.png</bigImg>
    </info>
    <warning>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>FDE97D</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>FDE97D</captionBgColor>
      <captionBgColorEnd>F09C08</captionBgColorEnd>
      <image>warning.png</image>
      <bigImg>warning.png</bigImg>
    </warning>
    <grey>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>E3E3E3</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>E3E3E3</captionBgColor>
      <captionBgColorEnd>ABABAB</captionBgColorEnd>
      <image>gears.png</image>
      <bigImg>gears.png</bigImg>
    </grey>
    <custom>
      <color>000000</color>
      <captionColor>FFFFFF</captionColor>
      <borderColor>F7CDF5</borderColor>
      <bgColor>FFFFFF</bgColor>
      <bgColorEnd>DDDDDD</bgColorEnd>
      <captionBgColor>F7CDF5</captionBgColor>
      <captionBgColorEnd>F77DF1</captionBgColorEnd>
      <image>heart.png</image>
      <bigImg>heart.png</bigImg>
    </custom>
  </cssStyles>
  <options>
    <side>1</side>
    <rounded_corners>true</rounded_corners>
    <border_style>none</border_style>
    <box_shadow>true</box_shadow>
    <js_shadow_enabled>true</js_shadow_enabled>
    <js_shadow_offsetX>0</js_shadow_offsetX>
    <js_shadow_offsetY>0</js_shadow_offsetY>
    <js_shadow_blur>15</js_shadow_blur>
    <js_shadow_color>555555</js_shadow_color>
    <js_radius>5</js_radius>
    <js_imgMinus>minus.png</js_imgMinus>
    <js_imgPlus>plus.png</js_imgPlus>
  </options>
</root>